{
  "name": "tinytemplatec",
  "type": "module",
  "version": "0.0.4",
  "description": "Lightweight, zero-dependency HTML template compiler with support for vue-like directives. Can be used for basic front-end frameworks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixldemon/tinytemplatec"
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "watch": "tsc -w --skipLibCheck"
  },
  "keywords": [
    "template engine",
    "template compiler",
    "front-end",
    "framework",
    "parser",
    "lightweight",
    "zero-dependency"
  ],
  "author": "Moritz Clerc <moritzamando@protonmail.com>",
  "license": "ISC",
  "devDependencies": {
    "typescript": "*"
  },
  "dependencies": {}
}
